<p id="notice"><%= notice %></p>

<h1>Relacion Entrada Unidads</h1>

<table>
  <thead>
    <tr>
      <th>Informe general</th>
      <th>Unidad</th>
      <th>Caducado</th>
      <th>Necesita</th>
      <th>Por caducar</th>
      <th>Existencia fisica</th>
      <th>Buen estado</th>
      <th>Extra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relacion_entrada_unidads.each do |relacion_entrada_unidad| %>
      <tr>
        <td><%= relacion_entrada_unidad.informe_general_id %></td>
        <td><%= relacion_entrada_unidad.unidad_id %></td>
        <td><%= relacion_entrada_unidad.caducado %></td>
        <td><%= relacion_entrada_unidad.necesita %></td>
        <td><%= relacion_entrada_unidad.por_caducar %></td>
        <td><%= relacion_entrada_unidad.existencia_fisica %></td>
        <td><%= relacion_entrada_unidad.buen_estado %></td>
        <td><%= relacion_entrada_unidad.extra %></td>
        <td><%= link_to 'Show', relacion_entrada_unidad %></td>
        <td><%= link_to 'Edit', edit_relacion_entrada_unidad_path(relacion_entrada_unidad) %></td>
        <td><%= link_to 'Destroy', relacion_entrada_unidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relacion Entrada Unidad', new_relacion_entrada_unidad_path %>
