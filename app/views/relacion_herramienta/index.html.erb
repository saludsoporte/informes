<p id="notice"><%= notice %></p>

<h1>Relacion Herramienta</h1>

<table>
  <thead>
    <tr>
      <th>Informe general</th>
      <th>Herramienta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relacion_herramienta.each do |relacion_herramientum| %>
      <tr>
        <td><%= relacion_herramientum.informe_general_id %></td>
        <td><%= relacion_herramientum.herramienta_id %></td>
        <td><%= link_to 'Show', relacion_herramientum %></td>
        <td><%= link_to 'Edit', edit_relacion_herramientum_path(relacion_herramientum) %></td>
        <td><%= link_to 'Destroy', relacion_herramientum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relacion Herramientum', new_relacion_herramientum_path %>
