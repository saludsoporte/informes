<div class="container">
<p id="notice"><%= notice %></p>

<h1>Informe Generals</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @informe_generals.each do |informe_general| %>
      <tr>
        <td><%= informe_general.nombre %></td>
        <td><%= link_to 'Show', informe_general %></td>
        <td><%= link_to 'Edit', edit_informe_general_path(informe_general) %></td>
        <td><%= link_to 'Destroy', informe_general, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Informe General', new_informe_general_path,class: :"color-letra" %>
<% @obj = @informe_generals %>

</div>